{
  "name": "v2.development.hiroshima-u.ac.jp",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "node ./CI+CD/convertYaml.js; vite build",
    "preview": "node ./CI+CD/convertYaml.js; vite build; vite preview --host",
    "check": "npx eslint --config ./.eslintrc.json ./src/ --ext .js,.vue",
